<view class="user-info"><view class="user-bg"></view><view class="user-main"><view class="user-head"><block ks:if="{{userInfo.user_image}}"><image src="{{userInfo.user_image}}"></image></block><block ks:else><image src="../../static/icons/default-head.png"></image></block><block ks:if="{{userInfo.user_id&&userId}}"><view class="have-login"><view><text>{{userInfo.mobile?userInfo.mobile:userInfo.name}}</text><block ks:if="{{time}}"><text class="icon-crown"></text></block></view><block ks:if="{{!userInfo.mobile}}"><text data-event-opts="{{[['tap',[['gotoBindPhone',['$event']]]]]}}" class="bind-phone" bindtap="__e">绑定手机号</text></block><block ks:if="{{time}}"><text class="vip-time">{{"会员到期："+time}}</text></block></view></block><block ks:else><block ks:if="{{!userInfo.user_id&&!userId}}"><view data-event-opts="{{[['tap',[['getUserInfo',['$event']]]]]}}" class="not-login" bindtap="__e"><text class="login-btn">登录/注册</text></view></block></block></view><block ks:if="{{flag==0}}"><view class="user-recharge"><text>{{"我的余额："+(userInfo.money?userInfo.money:0)+" 阅币"}}</text><text data-event-opts="{{[['tap',[['gotoRecharge',['$event']]]]]}}" class="recharge-btn" bindtap="__e">充值</text></view></block><block ks:if="{{flag==0}}"><view><navigator class="box-xf" url="../record/record?tab=xiaofei"><view><image src="../../static/icons/user-xf.png"></image><text>消费记录</text></view><text class="iconfont"></text></navigator><navigator class="box-xf" url="../record/record?tab=chongzhi"><view><image src="../../static/icons/user-cz.png"></image><text>充值记录</text></view><text class="iconfont"></text></navigator></view></block><navigator class="box-xf" url="../readlist/readlist"><view><image src="../../static/icons/user-sc.png"></image><text>最近阅读</text></view><text class="iconfont"></text></navigator><navigator class="box-xf" url="../collectlist/collectlist"><view><image src="../../static/icons/user-zx.png"></image><text>我的收藏</text></view><text class="iconfont"></text></navigator></view></view>