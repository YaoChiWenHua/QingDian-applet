<view class="rank data-v-2cbd39cc"><view class="rank-nav data-v-2cbd39cc"><view class="{{(('nav-tab data-v-2cbd39cc')+' '+(channel==='man'?'active-tab':''))}}" data-channel="man" data-event-opts="{{[['tap',[['selectTab',['$event']]]]]}}" onTap="__e"><text class="data-v-2cbd39cc">男频</text></view><view class="{{(('nav-tab data-v-2cbd39cc')+' '+(channel==='woman'?'active-tab':''))}}" data-channel="woman" data-event-opts="{{[['tap',[['selectTab',['$event']]]]]}}" onTap="__e"><text class="data-v-2cbd39cc">女频</text></view></view><view class="rank-main data-v-2cbd39cc"><view class="rank-left data-v-2cbd39cc"><block a:for="{{titlelist}}" a:for-item="item" a:for-index="index" a:key="index"><block class="data-v-2cbd39cc"><view class="{{(('left-nav data-v-2cbd39cc')+' '+(currentIndex===index?'active-nav':''))}}" data-index="{{index}}" data-id="{{item.id}}" data-event-opts="{{[['tap',[['selectTitle',['$event']]]]]}}" onTap="__e"><text class="data-v-2cbd39cc">{{item.title}}</text></view></block></block></view><block a:if="{{booklist.length>0}}"><view class="rank-right data-v-2cbd39cc"><scroll-view class="right-book data-v-2cbd39cc" style="{{'height:'+(scrollHeight+'rpx')+';'}}" scroll-y="{{true}}"><block a:for="{{booklist}}" a:for-item="item" a:for-index="index" a:key="index"><block class="data-v-2cbd39cc"><navigator class="book-item data-v-2cbd39cc" url="{{'../book/book?id='+item.book_id}}"><block a:if="{{item.book_id}}"><view class="book-style data-v-2cbd39cc"><image src="{{'http://img.haokanread.com/book/cover/'+item.book_id+'/m_'+item.cover}}" lazy-load="{{true}}" class="data-v-2cbd39cc"></image><view class="book-text data-v-2cbd39cc"><view class="title data-v-2cbd39cc">{{item.book_title}}</view><view class="text no-wrap-two data-v-2cbd39cc">{{item.description}}</view><view class="icon data-v-2cbd39cc"><text class="iconfont icon-geren data-v-2cbd39cc"></text>{{''+item.pen_name+''}}</view></view></view></block></navigator></block></block></scroll-view></view></block></view></view>