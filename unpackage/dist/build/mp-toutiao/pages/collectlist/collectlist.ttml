<view class="collect-list data-v-33a955f3" style="{{'margin-bottom:'+(selectColl?'98rpx':'0rpx')+';'}}"><view class="bookshelf-head data-v-33a955f3"><text class="data-v-33a955f3">{{"共 "+cTotal+" 本"}}</text><block class="data-v-33a955f3"><text data-event-opts="{{[['tap',[['selectCollShow',['$event']]]]]}}" class="edit-button data-v-33a955f3" bindtap="__e">{{selectColl?'取消':'编辑'}}</text></block></view><block tt:for="{{collectlist}}" tt:for-item="item" tt:for-index="index" tt:key="index"><block class="data-v-33a955f3"><view data-event-opts="{{[['tap',[['handleChecked',['$0'],[[['collectlist','',index]]]]]]]}}" class="item-book data-v-33a955f3" bindtap="__e"><view hidden="{{!(selectColl)}}" class="radio-btn data-v-33a955f3"><radio color="#ff5052" checked="{{item.checked}}" class="data-v-33a955f3"></radio></view><view class="read-book data-v-33a955f3"><text class="title data-v-33a955f3">{{item.book_title}}</text><text class="data-v-33a955f3">{{"作者："+item.pen_name}}</text></view></view></block></block><view hidden="{{!(selectColl)}}" class="delete-button data-v-33a955f3"><text data-event-opts="{{[['tap',[['selectCollHide',['$event']]]]]}}" class="cancel-btn data-v-33a955f3" bindtap="__e">取消</text><text data-event-opts="{{[['tap',[['deleteCollect',['$event']]]]]}}" bindtap="__e" class="data-v-33a955f3">删除</text></view></view>